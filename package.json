{
  "name": "jscommenter",
  "displayName": "JSCommenter",
  "description": "provide useful and interesting comments",
  "publisher": "Arno",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "repository": {
    "url": "https://github.com/SurfaceW/arno-vscode-vcomments-ext"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jscommenter.helloWorld"
  ],
  "main": "./out/extension.js",
  "category": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./src/snippets/snippet.json"
      },
      {
        "language": "typescript",
        "path": "./src/snippets/snippet.json"
      },
      {
        "language": "typescriptreact",
        "path": "./src/snippets/snippet.json"
      },
      {
        "language": "javascriptreact",
        "path": "./src/snippets/snippet.json"
      }
    ],
    "commands": [
      {
        "command": "jscommenter.insertComment",
        "title": "Insert Comment",
        "category": "Snippets"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3"
  }
}
